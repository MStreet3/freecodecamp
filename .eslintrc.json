{
    "env": {
        "node": true,
        "es6": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "rules": {
        "no-console": "off",
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ]
}